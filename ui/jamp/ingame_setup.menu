			hide			tabWindowNetPerf
			hide			tabWindowSound
			hide			applyChanges
			hide			applyPPChanges
			hide			applySoundChanges
			hide			vidrestart

			setitemcolor	setup_submenu forecolor .682 .682 1 1
			setitemcolor	tabButtonGameplay forecolor 1 0.2 0 1
		}
			
		onESC 
		{ 
			play				"sound/interface/button1"
			defer				VideoSetup	videowarningMenu

			close				ingame_setup
		}
		onClose
		{
