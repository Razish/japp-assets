#define EDITFIELD_BACKGROUND "clear"
//#define EDITFIELD_BACKGROUND "gfx/2d/select"

#define COMMON( n, g, t, r ) \
name n \
group g \
type t \
rect r \
mouseEnter { setitemrect highlight r } \
mouseExit { setitemrect highlight 0 0 0 0 }

#define DISABLED( n, g, t, r ) \
name n \
group g \
type t \
rect r \
decoration \
mouseEnter { setitemrect darklight r } \
mouseExit { setitemrect darklight 0 0 0 0 }

#define TEXT_DEFAULTS \
font 7 \
textscale .375 \
forecolor .615 .615 .956 1 \
backcolor 1 1 1 1 \
textstyle ITEM_TEXTSTYLE_SHADOWED

#define TEXT_APPLY \
font 7 \
textscale .5 \
forecolor 1 0 0 1

#define MISC_DEFAULTS \
border 0 \
visible 0

#define BORDER_DEFAULTS \
border 1 \
bordercolor .5 .5 .75 1.0 \
bordersize 0.25

#define SLIDER_DEFAULTS \
slider 92.0 20.0 8.0 20.0


#define BUTTON_DEFAULTS action { play "sound/interface/button1" }
#define BUTTON_CHANGEPP action { show applyPPChanges; play "sound/interface/button1" }
#define BUTTON_CHANGESOUND action { show applySoundChanges; play "sound/interface/button1" }

